{
  "extends": "eslint:recommended",
  "rules": {
      "no-alert": 1,
      "camelcase": 0,
      "curly": 1,
      "eqeqeq": 1,
      "no-console": 1,
      "guard-for-in": 1,
      "no-empty": 1,
      "no-param-reassign": 1,
      "no-unused-vars": 1,
      "parser": "babel-eslint",
      "quotes": [ 1, "single"]
  },
  "globals": {
    // any globals we want to allow will go here
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  }
}